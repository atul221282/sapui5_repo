{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es5",
            "es2015"
        ],
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "target": "es5"
    },
    "Files": [
        "*.ts"
    ],
    "exclude": [
        "node_modules"
    ],
}